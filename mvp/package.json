{
  "name": "intelligent-pr-assistant-mvp",
  "version": "1.0.0",
  "description": "MVP implementation of Intelligent PR Assistant for Atlassian",
  "main": "index.js",
  "scripts": {
    "start": "node backend/index.js",
    "dev": "nodemon backend/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "deploy:dev": "npm run build && aws lambda update-function-code --function-name pr-assistant-dev",
    "deploy:prod": "npm run build && aws lambda update-function-code --function-name pr-assistant-prod",
    "build": "webpack --mode production",
    "forge:install": "forge install",
    "forge:deploy": "forge deploy",
    "forge:tunnel": "forge tunnel"
  },
  "keywords": [
    "atlassian",
    "bitbucket",
    "jira",
    "pr-review",
    "ai",
    "devops"
  ],
  "author": "PR Assistant Team",
  "license": "MIT",
  "dependencies": {
    "@atlassian/forge-api": "^3.0.0",
    "@atlassian/forge-bridge": "^3.0.0",
    "@atlassian/forge-ui": "^1.0.0",
    "aws-sdk": "^2.1500.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "openai": "^4.20.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.8.0",
    "babel-loader": "^9.1.0",
    "eslint": "^8.52.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
